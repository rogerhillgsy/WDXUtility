{
    "name" : "Context Menu Demo",
    "version" : "1.0",
    "description" : "Edit XDW Question",
    "background" : {
        "scripts" : ["eventPage.js"],
        "persistent" : false
    },
    "content_scripts" : [{
      "matches": ["<all_urls>"],
      "js" : ["contentScript.js"]
    }],
    "permissions" : ["storage","notifications","contextMenus","activeTab"],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/WDX_16.png",
          "32": "images/WDX_32.png",
          "48": "images/WDX_48.png",
          "128": "images/WDX_128.png"
        }
      },
      "icons": {
        "16": "images/WDX_16.png",
        "32": "images/WDX_32.png",
        "48": "images/WDX_48.png",
        "128": "images/WDX_128.png"
    },
    "options_page" : "options.html",  
    "manifest_version" : 2
}